package com.capgemini.medicalcollection.database;

import java.util.ArrayList;
import java.util.LinkedHashSet;
import java.util.Set;

import com.capgemini.medicalcollection.bean.MedicineBean;

public class MedicineDb {

	static Set<MedicineBean> madlist = new LinkedHashSet<MedicineBean>();
	
	static {
		
		MedicineBean m1 = new MedicineBean(201,"Crocine",200,"Allopathic");
		MedicineBean m2 = new MedicineBean(202,"Paracetamol",250,"Allopathic");
		madlist.add(m1);
		madlist.add(m2);

	}

	public Set<MedicineBean> medDB() {

		return madlist;

	}

}
