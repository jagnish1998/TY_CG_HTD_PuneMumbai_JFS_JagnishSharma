package com.capgemini.springmvc.bean;

public class UserBean {
	private String empId;
	private String pass;
	
	//Getter And Setter
	public String getEmpId() {
		return empId;
	}
	public void setEmpId(String empId) {
		this.empId = empId;
	}
	public String getPass() {
		return pass;
	}
	public void setPass(String pass) {
		this.pass = pass;
	}
	
}
