<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Configuration for Set -->
	<bean id="sports" class="com.capgemini.springcore.beans.Sports">
		<property name="name" value="Cricket" />
		<property name="totalPlayer" value="11" />
		<property name="teamInfo">
			<map>
				<entry key="India" value="MS Dhoni" />
				<entry key="pakistan" value="Sarfaraz" />
				<entry key="Australia" value="Arron Finch"/>
			</map>
		</property>
	</bean>
</beans>

